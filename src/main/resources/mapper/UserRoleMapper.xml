<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bugull.locker.mapper.UserRoleMapper">
    <resultMap id="BaseResultMap" type="com.bugull.locker.entity.UserRoleEntity">
        <id property="id" javaType="string" column="id" />
        <result column="userId" jdbcType="VARCHAR" property="userId" />
        <result column="roleId" jdbcType="VARCHAR" property="roleId" />
    </resultMap>

    <sql id = "Base_Column_list">
        `id`, `userId`, `roleId`
    </sql>

    <insert id = "insert" parameterType="com.bugull.locker.entity.UserRoleEntity">
        INSERT INTO `sys_userrole` (`id`, `userId`, `roleId`)
        VALUES
        (#{id,jdbcType=VARCHAR}, #{userId,jdbcType=VARCHAR}, #{roleId,jdbcType=VARCHAR})
    </insert>

    <delete id="delete" parameterType="string">
        DELETE FROM `sys_userrole` WHERE `userId` = #{userId,jdbcType=VARCHAR}
    </delete>

</mapper>